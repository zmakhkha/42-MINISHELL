cat /etc/passwd | awk '{print $1}' : passes
awk -F: '{print $1}' /etc/passwd : passes


cat /etc/passwd | awk '{print $1 ' ' $2}' : passes
cat /etc/passwd | awk '{print $1 " " $2}' : passes
awk -F '{print $1 " " $2}' /etc/passwd : hangs on bash && minishell



awk -F: '{print "Username: " $1 ", UID: " $3}' /etc/passwd : passes
awk -F: '{print "Username: " $1 ", UID: " $3 ", GID: " $4 ", Home: " $6 ", Shell: " $7}' /etc/passwd : passes


awk -F: '$3 > 10 {print "Username: " $1 ", Home: " $6}' /etc/passwd : passes

ls -l $HOME | awk 'NR > 1 {sum += $5} END {print "Total Size: " sum " bytes"}' : passes
ls -l "$HOME" | awk 'NR > 1 {sum += $5} END {print "Total Size: " sum " bytes"}' : passes
ls -l '$HOME' | awk 'NR > 1 {sum += $5} END {print "Total Size: " sum " bytes"}' : passes
